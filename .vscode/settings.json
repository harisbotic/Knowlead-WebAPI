// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        //OS's and others things
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        
        //Auto generated things
        "**/project.lock.json": true,
        "**/bin": true,
        "**/obj": true,
        
        //Things that are in root
        "web.config": true,
        "Dockerfile": true,
        "README.md": true,
        "NuGet.Config": true,
        "global.json": true,
        "desktop.ini": true,
        "version.props": true,

        //Custom Scripts
        "run.bat": true,
        "seed.bat": true,
        "update.bat": true,
        "generate-dto.bat": true,
        "migrate.bat": true,
        "environment.bat": true,
        "auth.bat": true,

        //Git things
        ".gitignore": true,

        //Visual Studio things
        "**/*.sln": true,
        "**/*.userprefs": true,
        "**/*.csproj.user": true
        
    }
}